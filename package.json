{
  "publisher": "jiangjunfeng",
  "name": "jiangvscodeplugin",
  "displayName": "jiangVsCodePlugin",
  "description": "jiang study ",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jiangvscodeplugin.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "repository": "https://github.com/upJiang/jiang-vscode-plugin",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "tsc": "vue-tsc --noEmit --skipLibCheck",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vscode/test-electron": "^2.3.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.51.0",
    "eslint-plugin-prettier": "^5.0.1",
    "glob": "^10.3.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "galleryBanner": {
    "color": "#1890ff"
  },
  "viewsContainers": {
    "activitybar": [
      {
        "id": "jiang",
        "title": "jiang",
        "icon": "images/title.jpg"
      }
    ]
  }
}
